
@{
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
}
<div class="formbackgroud">
    <div class="layui-row index-header">
        <div class="pull-left">

        </div>

        <div class="pull-right">
            <div class="layui-btn-group">
                <button type="button" id="add" class="layui-btn layui-btn-sm"><i class="fa fa-plus"></i>&nbsp;新增</button>
                <button type="button" id="edit" class="layui-btn layui-btn-sm"><i class="fa fa-pencil"></i>&nbsp;编辑/查看</button>
                <button type="button" id="delete" class="layui-btn layui-btn-sm"><i class="fa fa-trash-o"></i>&nbsp;删除</button>
            </div>
            <div class="layui-btn-group">
                <button type="button" id="print" class="layui-btn layui-btn-sm"><i class="fa fa-print"></i>&nbsp;打印</button>
            </div>
            <div class="layui-btn-group">
                <button type="button" id="seeapproval" class="layui-btn layui-btn-sm"><i class="fa fa-link"></i>&nbsp;查看流程</button>
                <button type="button" id="revoke" class="layui-btn layui-btn-sm"><i class="fa fa-mail-reply-all"></i>&nbsp;生效撤回</button>
            </div>
        </div>
    </div>
    <div class="layui-row">
        <table id="grid"></table>
        <div id="gridPager"></div>
    </div>
</div>

@section Footer{
    @Html.AppendJs("~/Areas/OAModule/Views/Leave/Index.js")
}

